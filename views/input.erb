<div class="row">
  <div class="col-sm-8">
    <div id='createContact' class="well">
      <h1>Add contact information.</h1>
      <form action="/output" method="post">
        <div class="form-group">
          <label for="first_name">First name:</label>
          <input id="first" name="first_name" class="form-control" type="text">
        </div>
        <div class="form-group">
          <label for="last_name">Last name:</label>
          <input id="last" name="last_name" class="form-control" type="text">
        </div>
        <div class="form-group">
          <label for="number">Phone Number:</label>
          <input id="number" name="number" class="form-control" type="text">
        </div>
        <button type="submit" class="btn">Add!</button>
      </form>
    </div>
  </div>

  <div class="col-sm-4">
    <div id='displayContacts' class='well'>
      <h3>Contacts:</h3>
      <% @contacts.each do |contact| %>
        <h4><a href="/output/<%= contact.last_name %>"><%= contact.first_name %> <%= contact.last_name %></a></h4>
      <% end %>
    </div>
  </div>
</div>
